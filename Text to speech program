import pyttsx3
pyobj=pyttsx3.init()
fo=open("C:\\Users\\HARSH TRIPATHI\\Desktop\\Text to speech converter\\USEME.txt","r")
ip=fo.read()
fo.close()
pyobj.setProperty("rate",200)
pyobj.setProperty("volume",1)
pyobj.save_to_file(ip,"C:\\Users\\HARSH TRIPATHI\\Desktop\\Text to speech converter\\USEME.mp3")
pyobj.runAndWait()
